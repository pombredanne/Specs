{
  "name": "ffmpeg-kit-srt",
  "version": "1.0.2",
  "summary": "FFmpeg Kit iOS Https Shared Framework",
  "description": "Includes FFmpeg with gmp and gnutls libraries enabled.",
  "authors": {
    "sxudan": "sudosuwal@gmail.com"
  },
  "homepage": "https://github.com/sxudan/ffmpeg-kit-srt",
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/ios-arm64_arm64e/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "ios": "12.1"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/sxudan/ffmpeg-kit-srt-6.0-ios-xcframework.git",
    "tag": "1.0.0"
  },
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "ios": {
    "preserve_paths": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework"
    ]
  },
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "CoreMedia",
    "VideoToolbox"
  ],
  "vendored_frameworks": [
    "ffmpegkit.xcframework",
    "libavcodec.xcframework",
    "libavdevice.xcframework",
    "libavfilter.xcframework",
    "libavformat.xcframework",
    "libavutil.xcframework",
    "libswresample.xcframework",
    "libswscale.xcframework"
  ]
