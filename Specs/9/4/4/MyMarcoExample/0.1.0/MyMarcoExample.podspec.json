{
  "name": "MyMarcoExample",
  "version": "0.1.0",
  "summary": "A proof of concept macro to show they can work with cocoapods.",
  "description": "A proof of concept macro to show they can work with cocoapods.",
  "homepage": "https://github.com/RayJiang16/MyMacroExample",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "RayJiang16": "is.rayjiang@outlook.com"
  },
  "source": {
    "git": "https://github.com/RayJiang16/MyMacroExample",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": [
    "Sources/MyMacro/**/*"
  ],
  "swift_versions": "5.9",
  "preserve_paths": [
    "macros/MyMacroMacros"
  ],
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-load-plugin-executable ${PODS_ROOT}/MyMarcoExample/macros/MyMacroMacros#MyMacroMacros"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-load-plugin-executable ${PODS_ROOT}/MyMarcoExample/macros/MyMacroMacros#MyMacroMacros"
  },
  "swift_version": "5.9"
}
