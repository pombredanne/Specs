{
  "name": "OpenAdx",
  "version": "2.0.1",
  "summary": "OpenAdx广告平台",
  "description": "OpenAdx,广告SDK,自有直客广告主",
  "homepage": "https://github.com/konnitiha/OpenAdxSDK.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "openad": "1053591151@qq.com"
  },
  "source": {
    "git": "https://github.com/konnitiha/OpenAdxSDK.git",
    "tag": "2.0.1"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit",
    "MobileCoreServices",
    "CoreGraphics",
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "AdSupport",
    "StoreKit",
    "AVFoundation",
    "MediaPlayer",
    "CoreMedia",
    "WebKit",
    "Accelerate",
    "CoreLocation",
    "AVKit",
    "CoreMotion"
  ],
  "source_files": "OpenAdxSDK/OpenAdx.framework/Headers/*.{h,m}",
  "resources": [
    "OpenAdxSDK/OpenAdx.bundle"
  ],
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-lObjC"
    ],
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "libraries": [
    "z",
    "resolv.9",
    "sqlite3",
    "c++",
    "c++abi",
    "resolv"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 arm64"
  },
  "ios": {
    "vendored_frameworks": "OpenAdxSDK/OpenAdx.framework"
  }
}
