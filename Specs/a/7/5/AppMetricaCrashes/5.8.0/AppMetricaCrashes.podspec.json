  "summary": "Provides essential utilities for efficient crash and error handling and reporting",
  "homepage": "https://appmetrica.io",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "AppMetrica": "admin@appmetrica.io"
  },
  "source": {
