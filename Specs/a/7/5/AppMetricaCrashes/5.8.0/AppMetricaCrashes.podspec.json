    "AppMetricaCoreUtils": [
      "= 5.8.0"
    ],
    "AppMetricaHostState": [
      "= 5.8.0"
    ],
    "AppMetricaProtobufUtils": [
      "= 5.8.0"
    ],
    "AppMetricaLog": [
      "= 5.8.0"
    ],
    "AppMetricaPlatform": [
      "= 5.8.0"
    ],
    "AppMetricaStorageUtils": [
      "= 5.8.0"
    ],
    "AppMetricaEncodingUtils": [
      "= 5.8.0"
    ],
    "KSCrash/Recording": [
      ">= 2.0.0-rc.1",
      "< 2.1.0"
    ]
  },
  "header_dir": "AppMetricaCrashes",
  "source_files": "AppMetricaCrashes/Sources/**/*.{h,m,c}",
