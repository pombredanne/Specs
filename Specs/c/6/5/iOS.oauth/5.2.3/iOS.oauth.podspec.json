{
  "name": "iOS.oauth",
  "version": "5.2.3",
  "summary": "OAuth2 framework for iOS",
  "description": "A flexible framework supporting standards-compliant _implicit_ and _code_ grant flows. Some\nwebsites like Facebook may use slightly differring OAuth2 implementations, for those the\nframework aims to provide specific subclasses handling these differences.",
  "homepage": "https://git.delta.com/enterprisemobility/ios-oauth-enterprise",
  "documentation_url": "https://git.delta.com/enterprisemobility/ios-oauth-enterprise",
  "license": "Internal",
  "authors": {
    "vishnu": "vishnu.tunguturi@delta.com"
  },
  "platforms": {
    "ios": "12.0",
    "osx": "10.13"
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-DNO_MODULE_IMPORT -DNO_KEYCHAIN_IMPORT"
  },
  "source": {
    "git": "https://git.delta.com/enterprisemobility/ios-oauth-enterprise.git",
    "tag": "5.2.3"
  },
  "swift_versions": "5.0",
  "source_files": [
    "SwiftKeychain/Keychain/*.swift",
    "Sources/Base/*.swift",
    "Sources/Flows/*.swift",
    "Sources/DataLoader/*.swift"
  ],
  "ios": {
    "source_files": "Sources/iOS/*.swift",
    "frameworks": "SafariServices"
  },
  "osx": {
    "source_files": "Sources/macOS/*.swift"
  },
  "swift_version": "5.0"
}
