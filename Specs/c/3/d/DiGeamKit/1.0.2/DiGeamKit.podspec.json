{
  "name": "DiGeamKit",
  "version": "1.0.2",
  "summary": "DiGeamKit工具",
  "homepage": "https://gitee.com/jinyi.com/di-geam-kit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "DF": "993942283@qq.com"
  },
  "source": {
    "git": "https://gitee.com/jinyi.com/di-geam-kit.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "requires_arc": true,
  "preserve_paths": "DiGeamKit.framework",
  "vendored_frameworks": "DiGeamKit.framework",
  "default_subspecs": "Default",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/DiGeamKit/DiGeamKit.framework/Headers/"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "MJExtension": [
      "~> 3.4.1"
    ],
    "MBProgressHUD": [
      "~> 1.2.0"
    ],
    "LSTPopView": [
      "~> 0.3.10"
    ],
    "LSTTimer": [
      "~> 0.2.10"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "Default",
      "source_files": "DiGeamKit.framework/**/*.{h}",
      "resources": "DiGeamKit.framework/*.{bundle}"
    }
  ]
}
