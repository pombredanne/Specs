  "platforms": {
    "ios": "13.0",
    "osx": "10.15"
  },
  "source": {
    "git": "https://github.com/HituziANDO/LoggingViewKit.git",
    "tag": "6.1.6"
  },
  "source_files": "LoggingViewKit/LoggingViewKit/**/*.{h,m,swift}",
  "ios": {
    "exclude_files": [
      "Frameworks/*",
      "Sample/*"
    ],
    "frameworks": "UIKit"
