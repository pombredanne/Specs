{
  "name": "ReerRouter",
  "version": "2.0.1",
  "summary": "A router for iOS app.",
  "description": "App URL router for iOS (Swift only).",
  "homepage": "https://github.com/reers/ReerRouter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "phoenix": "x.rhythm@qq.com"
  },
  "source": {
    "git": "https://github.com/reers/ReerRouter.git",
    "tag": "2.0.1"
  },
  "swift_versions": "5.10",
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "Sources/**/*",
  "preserve_paths": [
    "Sources/Resources/ReerRouterMacros"
  ],
  "exclude_files": "Sources/ReerRouterMacros",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/ReerRouter/Sources/Resources/ReerRouterMacros#ReerRouterMacros"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/ReerRouter/Sources/Resources/ReerRouterMacros#ReerRouterMacros"
  },
  "swift_version": "5.10"
}
