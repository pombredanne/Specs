{
  "name": "APExtensions",
  "version": "14.2.0",
  "summary": "A helpful collection of extensions, controllers and protocols.",
  "description": "A helpful collection of extensions, controllers and protocols. See documentation for details.",
  "homepage": "https://github.com/APUtils/APExtensions",
  "documentation_url": "https://aputils.github.io/APExtensions/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Anton Plebanovich": "anton.plebanovich@gmail.com"
  },
  "source": {
    "git": "https://github.com/APUtils/APExtensions.git",
    "tag": "14.2.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": [
    "5"
  ],
  "cocoapods_version": ">= 1.13.0",
  "default_subspecs": [
    "Core",
    "Dispatch",
    "Occupiable",
    "OptionalType",
    "ViewModel",
    "Storyboard"
  ],
  "subspecs": [
    {
      "name": "Core",
      "frameworks": [
        "Foundation",
        "UIKit",
        "MessageUI"
      ],
      "source_files": [
        "APExtensions/Classes/Core/**/*",
        "APExtensions/Classes/Shared/**/*"
      ],
      "resource_bundles": {
        "APExtensions.Core.privacy": "Pod/Privacy/APExtensions.Core/PrivacyInfo.xcprivacy"
      },
      "dependencies": {
        "RoutableLogger": [

        ]
      }
    },
    {
      "name": "ViewModel",
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "source_files": "APExtensions/Classes/ViewModel/**/*",
      "resource_bundles": {
        "APExtensions.ViewModel.privacy": "Pod/Privacy/APExtensions.ViewModel/PrivacyInfo.xcprivacy"
      }
    },
    {
      "name": "Storyboard",
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "source_files": [
        "APExtensions/Classes/Storyboard/**/*",
        "APExtensions/Classes/Shared/**/*"
      ],
      "resource_bundles": {
        "APExtensions.Storyboard.privacy": "Pod/Privacy/APExtensions.Storyboard/PrivacyInfo.xcprivacy"
      },
      "dependencies": {
        "RoutableLogger": [

        ]
      }
    },
    {
      "name": "Dispatch",
      "frameworks": "Foundation",
      "source_files": "APExtensions/Classes/Dispatch/**/*",
      "resource_bundles": {
        "APExtensions.Dispatch.privacy": "Pod/Privacy/APExtensions.Dispatch/PrivacyInfo.xcprivacy"
      },
      "dependencies": {
        "RoutableLogger": [

        ]
      }
    },
    {
      "name": "Occupiable",
      "platforms": {
        "ios": "11.0",
        "osx": "10.13",
        "watchos": "4.0",
        "tvos": "11.0"
      },
      "source_files": "APExtensions/Classes/Core/_Protocols/Occupiable/Occupiable.swift",
      "resource_bundles": {
        "APExtensions.Occupiable.privacy": "Pod/Privacy/APExtensions.Occupiable/PrivacyInfo.xcprivacy"
      }
    },
    {
      "name": "OptionalType",
      "platforms": {
        "ios": "11.0",
        "osx": "10.13",
        "watchos": "4.0",
        "tvos": "11.0"
      },
      "source_files": "APExtensions/Classes/Core/_Protocols/OptionalType/OptionalType.swift",
      "resource_bundles": {
        "APExtensions.OptionalType.privacy": "Pod/Privacy/APExtensions.OptionalType/PrivacyInfo.xcprivacy"
      }
    }
  ],
  "swift_version": "5"
}
