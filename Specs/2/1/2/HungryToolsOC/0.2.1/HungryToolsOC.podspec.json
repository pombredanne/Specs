{
  "name": "HungryToolsOC",
  "version": "0.2.1",
  "summary": "Some common tools",
  "description": "Some tools often used.",
  "homepage": "https://github.com/HungryZ/HungryToolsOC",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhanghaichuan": "924320752@qq.com"
  },
  "source": {
    "git": "https://github.com/HungryZ/HungryToolsOC.git",
    "tag": "0.2.1"
  },
  "user_target_xcconfig": {
    "GENERATE_INFOPLIST_FILE": "YES"
  },
  "pod_target_xcconfig": {
    "GENERATE_INFOPLIST_FILE": "YES"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Category",
  "subspecs": [
    {
      "name": "Macro",
      "source_files": "HungryToolsOC/Classes/Macro/**/*"
    },
    {
      "name": "Category",
      "source_files": "HungryToolsOC/Classes/Category/**/*"
    },
    {
      "name": "UITool",
      "source_files": "HungryToolsOC/Classes/UITool/**/*",
      "resource_bundles": {
        "Resource": [
          "HungryToolsOC/Assets/Resource/**/*"
        ]
      }
    }
  ]
}
