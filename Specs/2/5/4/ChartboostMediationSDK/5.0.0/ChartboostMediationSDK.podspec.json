{
  "name": "ChartboostMediationSDK",
  "version": "5.0.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "homepage": "https://github.com/ChartBoost/chartboost-mediation-ios-sdk",
  "authors": {
    "Chartboost": "https://www.chartboost.com/"
  },
  "summary": "Chartboost Mediation iOS SDK.",
  "description": "A programmatic ad monetization platform for mobile games and apps.",
  "documentation_url": "https://developers.chartboost.com",
  "module_name": "ChartboostMediationSDK",
  "source": {
    "git": "https://github.com/ChartBoost/chartboost-mediation-ios-sdk.git",
    "tag": "5.0.0"
  },
  "source_files": "Source/**/*.{swift,md}",
  "resource_bundles": {
    "ChartboostMediationSDK": [
      "PrivacyInfo.xcprivacy"
    ]
  },
  "static_framework": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "13.0"
  },
  "ios": {
    "frameworks": [
      "AVFoundation",
      "CoreGraphics",
      "StoreKit",
      "Foundation",
      "UIKit",
      "WebKit",
      "CoreTelephony",
      "AdSupport"
    ]
  },
  "dependencies": {
    "ChartboostCoreSDK": [
      "~> 1.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/**/*.{swift,h,m}",
      "resources": "Tests/Resources/**/*",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "14.0"
      },
      "info_plist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "test-0.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "test-1.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "test-2.skadnetwork"
          }
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
