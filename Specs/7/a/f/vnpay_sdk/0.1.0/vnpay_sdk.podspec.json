{
  "name": "vnpay_sdk",
  "version": "0.1.0",
  "summary": "a plugin project for vnpay.",
  "description": "vnpay_sdk is a plugin project for vnpay. It's a static framework. It's help you integrate vnpay into your project.",
  "homepage": "https://github.com/lumi-software/vnpay_sdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Lumi Viet Nam": "nghinv@lumi.vn"
  },
  "source": {
    "git": "https://github.com/lumi-software/vnpay_sdk.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "Classes/**/*",
  "public_header_files": "Classes/**/*.h",
  "vendored_frameworks": "Frameworks/CallAppSDK.framework",
  "resource_bundles": {
    "vnpay_sdk": [
      "Assets/*.png",
      "Assets/*.imageset"
    ]
  },
  "resources": [
    "Assets/*.png",
    "Assets/*.imageset"
  ],
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
