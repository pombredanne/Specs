{
  "name": "VisionCCiOSSDK",
  "version": "1.6.3.1",
  "summary": "VisionCCiOSSDK是维音CC客服系统在iOS平台访客端的解决方案，既包含了客服聊天逻辑管理，也提供了聊天界面，开发者可方便的将客服功能集成到自己的 App 中",
  "homepage": "https://github.com/visioncc-vxi/visioncc-ios-sdk",
  "license": "MIT",
  "authors": {
    "vxi": "esbu@vxichina.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/visioncc-vxi/visioncc-ios-sdk.git",
    "tag": "1.6.3.1"
  },
  "frameworks": "UIKit",
  "requires_arc": true,
  "ios": {
    "vendored_frameworks": "VisionCCiOSSDK.xcframework"
  },
  "prepare_command": "cp -rf 'VisionCCiOSSDKThem.bundle' 'VisionCCiOSSDK.xcframework/ios-arm64/VisionCCiOSSDK.framework/'\ncp -rf 'VisionCCiOSSDKThem.bundle' 'VisionCCiOSSDK.xcframework/ios-arm64_x86_64-simulator/VisionCCiOSSDK.framework/'",
  "dependencies": {
    "RxSwift": [
      "6.5.0"
    ],
    "Alamofire": [
      "5.7.1"
    ],
    "SnapKit": [
      "5.6.0"
    ],
    "RxCocoa": [
      "6.5.0"
    ],
    "Starscream": [
      "4.0.6"
    ],
    "Socket.IO-Client-Swift": [
      "~> 16.1.0"
    ],
    "JXSegmentedView": [
      "1.2.7"
    ],
    "NSObject+Rx": [
      "5.2.2"
    ],
    "SwiftSVG": [
      "~> 2.3.2"
    ],
    "ZLPhotoBrowser": [
      "4.5.3"
    ],
    "MJRefreshSwift": [
      "1.5.14"
    ],
    "IQKeyboardManagerSwift": [
      "6.5.11"
    ]
  },
  "user_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "swift_version": "5.0"
}
