{
  "name": "Moocher",
  "version": "0.4.2",
  "summary": "A set of \"rspec-like\" test matchers that \"mooch\" off of XCTest",
  "homepage": "https://github.com/rbaumbach/Moocher",
  "license": {
    "type": "MIT",
    "file": "MIT-LICENSE.txt"
  },
  "authors": {
    "Ryan Baumbach": "github@ryan.codes"
  },
  "source": {
    "git": "https://github.com/rbaumbach/Moocher.git",
    "tag": "0.4.2"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": "XCTest",
  "swift_versions": "5.5",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Moocher/**/*.{swift}"
    },
    {
      "name": "Polling",
      "source_files": "Sources/MoocherPolling/**/*.{swift}"
    }
  ],
  "swift_version": "5.5"
}
