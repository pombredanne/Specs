{
  "name": "XZJSON",
  "version": "1.0.0",
  "summary": "XZJSON 是一款高效的 JSON 数据模型化工具",
  "description": "XZJSON 基于 YYModel 打造，支持任意数据的模型化；采用了工具类+协议的方式实现，接入更方便。",
  "homepage": "https://github.com/Xezun/XZJSON",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Xezun": "developer@xezun.com"
  },
  "source": {
    "git": "https://github.com/Xezun/XZJSON.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "XZ_FRAMEWORK=1"
  },
  "subspecs": [
    {
      "name": "Code",
      "source_files": "XZJSON/Code/**/*.{h,m}",
      "private_header_files": "XZJSON/Code/**/Objc/*.h",
      "project_header_files": "XZJSON/Code/**/Private/*.h"
    }
  ]
}
