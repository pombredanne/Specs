    "c++",
    "sqlite3",
    "xml2",
    "bz2",
    "c++abi",
    "resolv.9",
    "iconv"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "AVKit",
    "Accelerate",
    "AudioToolbox",
    "CoreData",
    "CoreLocation",
    "CoreMedia",
    "CoreMotion",
    "CoreGraphics",
    "CoreTelephony",
    "Foundation",
    "MediaPlayer",
    "MessageUI",
    "MobileCoreServices",
    "MapKit",
    "QuartzCore",
    "QuickLook",
