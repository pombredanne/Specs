    "Security",
    "StoreKit",
    "WebKit",
    "UIKit",
    "ImageIO",
    "SafariServices",
    "JavaScriptCore",
    "CoreImage",
    "CoreText"
  ],
  "xcconfig": {
    "VALID_ARCHS": "armv7 armv7s x86_64 arm64",
    "OTHER_LDFLAGS": "-ObjC"
