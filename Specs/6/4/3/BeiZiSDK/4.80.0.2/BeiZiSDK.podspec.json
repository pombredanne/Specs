    "lvjunxue": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://code.aliyun.com/lvjunxue/BeiZiSDK.git",
    "tag": "4.80.0.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "libraries": [
