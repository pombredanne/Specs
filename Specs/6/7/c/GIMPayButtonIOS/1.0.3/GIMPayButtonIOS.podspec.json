{
  "name": "GIMPayButtonIOS",
  "version": "1.0.3",
  "summary": "GIM PayButton SDK",
  "description": "PayButton SDK helps make the integration of card acceptance into your app easy.",
  "homepage": "https://github.com/GIMPAY/paybutton-sdk-ios",
  "license": "MIT",
  "authors": {
    "Paysky Company": "amir.morsy@paysky.io"
  },
  "social_media_url": "https://paysky.io/",
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/GIMPAY/paybutton-sdk-ios.git",
    "tag": "1.0.3"
  },
  "swift_versions": "5.0",
  "public_header_files": "PayButton/PayButton-Bridging-Header.h",
  "frameworks": "UIKit",
  "requires_arc": true,
  "static_framework": true,
  "xcconfig": {
    "APPLY_RULES_IN_COPY_FILES": "YES",
    "STRINGS_FILE_OUTPUT_ENCODING": "binary",
    "OTHER_LDFLAGS": "-lz"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source_files": "PayButton/**/*.{swift,h,m}",
  "resources": "PayButton/**/*.{png,jpeg,jpg,storyboard,xib,xcassets,lproj,json,plist,strings}",
  "resource_bundles": {
    "PayButton": [
      "PayButton/Resources/*.lproj/*.strings"
    ]
  },
  "exclude_files": [
    "PayButton/TestApi/Base.lproj/LaunchScreen.storyboard",
    "PayButton/TestApi/Base.lproj/Main.storyboard",
    "PayButton/TestApi/ViewController.swift",
    "PayButton/AppDelegate.swift",
    "PayButton/Info.plist",
    "PayButton/PayButtonAssets.xcassets/AppIcon.appiconset/**"
  ],
  "dependencies": {
    "MOLH": [

    ],
    "Alamofire": [

    ],
    "PayCardsRecognizer": [

    ]
  },
  "swift_version": "5.0"
}
