{
  "name": "DDLoggerSwift",
  "swift_versions": "5.0",
  "version": "5.0.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "The iOS side displays the output log log on the screen, and can generate log file sharing, which is convenient for debugging information",
  "homepage": "https://github.com/DamonHu/DDLoggerSwift",
  "authors": {
    "ZXKitCode": "dong765@qq.com"
  },
  "source": {
    "git": "https://github.com/DamonHu/DDLoggerSwift.git",
    "tag": "5.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "documentation_url": "https://ddceo.com/blog/1305.html",
  "default_subspecs": "core",
  "subspecs": [
    {
      "name": "core",
      "resource_bundles": {
        "DDLoggerSwift": [
          "pod/assets/**/*"
        ]
      },
      "libraries": "sqlite3",
      "source_files": [
        "pod/*.swift",
        "pod/view/*.swift"
      ],
      "dependencies": {
        "DDUtils/ui": [
          "~>5.0.0"
        ],
        "DDUtils/utils": [
          "~>5.0.0"
        ]
      }
    },
    {
      "name": "socket",
      "dependencies": {
        "DDLoggerSwift/core": [

        ],
        "CocoaAsyncSocket": [

        ]
      },
      "source_files": "pod/socket/*.swift"
    }
  ],
  "swift_version": "5.0"
}
