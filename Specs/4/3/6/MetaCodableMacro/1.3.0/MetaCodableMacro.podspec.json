      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/AnyInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/AnyRequiredVariableInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/IgnoredInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/MemberwiseInitGenerator.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/OptionalInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/RequiredInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/RequiredInitializationWithDefaultValue.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/RequiredVariableInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Initialization/VariableInitialization.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Property/AliasedPropertyVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Property/AnyPropertyVariable.swift",
