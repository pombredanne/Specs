      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Property/KeyedVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Property/PropertyVariable.swift",
