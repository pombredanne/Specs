      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/GroupedVariableDeclaration.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/InvalidCombination.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/InvalidDeclaration.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/MetaCodableMessage.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/StaticVariableDeclaration.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/UninitializedVariableDecl.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Expansion/AttributeExpander.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Expansion/Options/DeclModifiersGenerator.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Expansion/Options/Options.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/ComposedVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/ConditionalCodingVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/AdjacentlyTaggableSwitcher.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/AdjacentlyTaggedEnumSwitcher.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/AnyEnumSwitcher.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/AssociatedVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/BasicAssociatedVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/BasicEnumCaseVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Enum/EnumCaseVariable.swift",
