      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Syntax/MemberGroupSyntax.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Syntax/PropertyDeclSyntax.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Syntax/VariableSyntax.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/ActorVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/ClassVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/Data/CodingKeysMap/Case.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/Data/CodingKeysMap/CodingKeysMap.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/Data/CodingKeysMap/Key.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/Data/ConstraintGenerator.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/Data/Registration.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/EnumVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/MemberGroup.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Type/TypeVariable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Variables/Variable.swift",
      "$(PODS_TARGET_SRCROOT)/Utils/macro_plugin_build.sh"
    ],
    "output_files": [
      "$(PODS_BUILD_DIR)/Macros/MacroPlugin/release/MacroPlugin"
    ],
    "execution_position": "before_compile"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xfrontend -load-plugin-executable -Xfrontend ${PODS_BUILD_DIR}/Macros/MacroPlugin/release/MacroPlugin#MacroPlugin",
    "METACODABLE_PLUGIN_BUILD_ENVIRONMENT": "METACODABLE_BEING_USED_FROM_COCOAPODS=true"
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xfrontend -load-plugin-executable -Xfrontend ${PODS_BUILD_DIR}/Macros/MacroPlugin/release/MacroPlugin#MacroPlugin",
    "METACODABLE_PLUGIN_BUILD_ENVIRONMENT": "METACODABLE_BEING_USED_FROM_COCOAPODS=true"
  },
  "swift_version": "5.9"
}
