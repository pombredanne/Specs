{
  "name": "MetaCodableMacro",
  "module_name": "MetaCodable",
  "version": "1.3.0",
  "homepage": "https://github.com/SwiftyLab/MetaCodable",
  "summary": "Supercharge Swift's Codable implementations with macros.",
  "description": "MetaCodable exposes custom macros which can be used to generate dynamic Codable implementations.",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "social_media_url": "https://twitter.com/soumya_mahunt",
  "readme": "https://github.com/SwiftyLab/MetaCodable/blob/v1.3.0/README.md",
  "changelog": "https://github.com/SwiftyLab/MetaCodable/blob/v1.3.0/CHANGELOG.md",
  "documentation_url": "https://swiftpackageindex.com/SwiftyLab/MetaCodable/v1.3.0/documentation/metacodable",
  "source": {
    "git": "https://github.com/SwiftyLab/MetaCodable.git",
    "tag": "v1.3.0"
  },
  "authors": {
    "Soumya Ranjan Mahunt": "soumya.mahunt@gmail.com"
  },
  "swift_versions": "5.9",
  "platforms": {
    "ios": "13.0",
    "osx": "10.15",
    "tvos": "13.0",
    "watchos": "6.0"
  },
  "preserve_paths": [
    "*.md",
    "LICENSE",
    "Package.swift",
    "Sources/{MacroPlugin,PluginCore}",
    "Utils/macro_plugin_build.sh"
  ],
  "source_files": "Sources/MetaCodable/**/*.*",
  "script_phases": {
    "name": "Build MetaCodable macro plugin",
    "script": "  echo \"env -i PATH=\\\"$PATH\\\" SRCROOT=\\\"$PODS_TARGET_SRCROOT\\\" BUILD_DIR=\\\"$PODS_BUILD_DIR\\\" TOOLCHAIN=\\\"$DT_TOOLCHAIN_DIR\\\" CONFIG=release $METACODABLE_PLUGIN_BUILD_ENVIRONMENT \\\"${PODS_TARGET_SRCROOT}/Utils/macro_plugin_build.sh\\\"\"\n  env -i PATH=\"$PATH\" SRCROOT=\"$PODS_TARGET_SRCROOT\" BUILD_DIR=\"$PODS_BUILD_DIR\" TOOLCHAIN=\"$DT_TOOLCHAIN_DIR\" CONFIG=release $METACODABLE_PLUGIN_BUILD_ENVIRONMENT \"${PODS_TARGET_SRCROOT}/Utils/macro_plugin_build.sh\"\n",
    "input_files": [
      "$(PODS_TARGET_SRCROOT)/Package.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/MacroPlugin/Definitions.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/MacroPlugin/Plugin.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/AttributableDeclSyntax.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Attribute.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/Codable+Expansion.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/Codable.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/CodingKeys/CodingKeyTransformer.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/CodingKeys/CodingKeys.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/CodingKeys/Strategy.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Codable/IgnoreCodingInitialized.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/CodedAs.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/CodedBy.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/Default.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/IgnoreCoding/IgnoreCoding.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/IgnoreCoding/IgnoreDecoding.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/IgnoreCoding/IgnoreEncoding.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/KeyPath/CodedAt.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/KeyPath/CodedIn.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/KeyPath/ContentAt.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/KeyPath/KeyPathProvider.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/MemberInit/MemberInit+Expansion.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/MemberInit/MemberInit.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Attributes/PeerAttribute.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/AggregatedDiagnosticProducer.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/CombinedUsage.swift",
      "$(PODS_TARGET_SRCROOT)/Sources/PluginCore/Diagnostics/Condition/ArgumentCountCondition.swift",
