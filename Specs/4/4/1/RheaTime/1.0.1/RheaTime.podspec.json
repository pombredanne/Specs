{
  "name": "RheaTime",
  "version": "1.0.1",
  "summary": "iOS App Time Dispatcher.",
  "description": "iOS App Time Dispatcher (Swift, Objc supported).",
  "homepage": "https://github.com/reers/Rhea",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Asura19": "x.rhythm@qq.com"
  },
  "source": {
    "git": "https://github.com/reers/Rhea.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.10",
  "source_files": "Sources/**/*",
  "preserve_paths": [
    "Sources/Resources/RheaTimeMacros"
  ],
  "exclude_files": "Sources/RheaTimeMacros",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-load-plugin-executable ${PODS_ROOT}/RheaTime/Sources/Resources/RheaTimeMacros#RheaTimeMacros -enable-experimental-feature SymbolLinkageMarkers"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-load-plugin-executable ${PODS_ROOT}/RheaTime/Sources/Resources/RheaTimeMacros#RheaTimeMacros -enable-experimental-feature SymbolLinkageMarkers"
  },
  "swift_version": "5.10"
}
