{
  "name": "RheaTime",
  "version": "1.1.0",
  "summary": "iOS App Time Dispatcher.",
  "description": "iOS App Time Dispatcher (Swift, Objc supported).",
  "homepage": "https://github.com/reers/Rhea",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Asura19": "x.rhythm@qq.com"
  },
  "source": {
    "git": "https://github.com/reers/Rhea.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15",
    "watchos": "7.0",
    "tvos": "13.0",
    "visionos": "1.0"
  },
  "swift_versions": "5.10",
  "source_files": "Sources/**/*",
  "preserve_paths": [
    "Sources/Resources/RheaTimeMacros"
  ],
  "exclude_files": "Sources/RheaTimeMacros",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/RheaTime/Sources/Resources/RheaTimeMacros#RheaTimeMacros"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/RheaTime/Sources/Resources/RheaTimeMacros#RheaTimeMacros"
  },
  "swift_version": "5.10"
}
