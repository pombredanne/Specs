{
  "name": "M3U8Kit",
  "version": "1.1.0",
  "summary": "A light weight m3u8 parser.",
  "homepage": "https://github.com/M3U8Kit/M3U8Parser.git",
  "license": "MIT",
  "authors": "M3U8Kit",
  "source": {
    "git": "https://github.com/M3U8Kit/M3U8Parser.git",
    "tag": "1.1.0"
  },
  "cocoapods_version": ">= 1.12.0",
  "platforms": {
    "ios": "12.0",
    "osx": "10.13",
    "tvos": "12.0",
    "watchos": "4.0"
