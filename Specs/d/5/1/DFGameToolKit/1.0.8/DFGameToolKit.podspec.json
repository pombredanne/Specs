{
  "name": "DFGameToolKit",
  "version": "1.0.8",
  "summary": "封装SDK上传Cocoapods的DFGameToolKit工具集.",
  "homepage": "https://github.com/WuFuPeiQian/DFGameToolKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "DF": "993942283@qq.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/WuFuPeiQian/DFGameToolKit.git",
    "tag": "1.0.8"
  },
  "requires_arc": true,
  "preserve_paths": "HLTestSDK.framework",
  "source_files": "HLTestSDK.framework/Headers/*.h",
  "public_header_files": "HLTestSDK.framework/Headers/*.h",
  "vendored_frameworks": "HLTestSDK.framework",
  "resource_bundles": {
    "Resource": [
      "HLTestSDK.framework/Resource.bundle/Resource/*.jpg"
    ]
  },
  "resources": "HLTestSDK.framework/Resource.bundle/Resource/*.jpg",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/DFGameToolKit/HLTestSDK.framework/Headers/"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "MJExtension": [
      "~> 3.4.1"
    ],
    "MBProgressHUD": [
      "~> 1.2.0"
    ],
    "LSTPopView": [
      "~> 0.3.10"
    ],
    "LSTTimer": [
      "~> 0.2.10"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
