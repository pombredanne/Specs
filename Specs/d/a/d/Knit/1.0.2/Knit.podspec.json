{
  "name": "Knit",
  "version": "1.0.2",
  "summary": "A tool for adding safety features to Swinject",
  "description": "Knit parses Swinject code and generates Swift files for type safety and unit testing.",
  "homepage": "https://github.com/cashapp/knit",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Cash App": "https://github.com/cashapp"
  },
  "source": {
    "http": "https://github.com/cashapp/knit/releases/download/1.0.2/knit-1.0.2.zip"
  },
  "source_files": "Sources/Knit/**/*.swift",
  "preserve_paths": "*",
  "platforms": {
    "ios": "15.0"
  },
  "dependencies": {
