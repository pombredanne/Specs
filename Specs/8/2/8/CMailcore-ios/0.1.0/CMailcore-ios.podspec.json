{
  "name": "CMailcore-ios",
  "version": "0.1.0",
  "summary": "Rebuild Mailcore for iOS with mailcore2-ios",
  "description": "MailCore provide a simple and asynchronous API to work with e-mail protocols IMAP, POP and SMTP. The API has been redesigned from ground up.",
  "homepage": "https://github.com/MailCore/mailcore2",
  "license": "MIT",
  "authors": "MailCore Authors",
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/driverZhan/mailcore-ios.git",
    "tag": "0.1.0"
  },
  "header_dir": "MailCore",
  "requires_arc": false,
  "source_files": "MailCore/include/MailCore/*.h",
  "vendored_libraries": "MailCore/lib/libMailCore-ios.a",
  "libraries": [
    "xml2",
    "iconv",
    "z",
    "c++",
    "resolv"
  ]
}
