{
  "name": "Dalila",
  "version": "1.0.3",
  "summary": "Dalila Module",
  "description": "Dalila iOS App is an application powered by TAM, designed to provide users with a comprehensive platform for various functionalities.",
  "homepage": "https://github.com/Saudi-Tourism-Authority/Dalila-iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ahmed Hussein": "ahmed.hussein@tamhub.com"
  },
  "platforms": {
    "ios": "16.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "git@github.com:Saudi-Tourism-Authority/Dalila-iOS.git",
    "tag": "1.0.3"
  },
  "source_files": "Dalila/Dalila/Source/**/*.{h,m,swift}",
  "resource_bundles": {
    "Dalila": [
      "Dalila/Dalila/Source/**/*.xib",
      "Dalila/Dalila/Source/Resources/**/*.{xcassets,strings,geojson}",
      "Dalila/Dalila/Source/Resources/Fonts/**/*",
      "Dalila/Dalila/Source/Configurations/Config Files/Config-*.plist"
    ]
  },
  "pod_target_xcconfig": {
    "BUNDLE_ID": "tamhub.com.dalila",
    "APP_NAME": "Dalila",
    "BASE_DOMAIN": "stg.api.dalila.tam.codes",
    "BASE_REST_DOMAIN": "stg.api.dalila.tam.codes",
    "PAGE_SIZE": "50",
    "PORT": "80",
    "TIMEOUT_INTERVAL": "120",
    "PROTOCOL": "https",
    "TOKEN_EXPIRATION_CODE": "403"
  },
  "info_plist": {
    "BaseDomain": "$(BASE_DOMAIN)",
    "BaseRestDomain": "$(BASE_REST_DOMAIN)",
    "PageSize": "$(PAGE_SIZE)",
    "Port": "$(PORT)",
    "TimeoutInterval": "$(TIMEOUT_INTERVAL)",
    "URLProtocol": "$(PROTOCOL)"
  },
  "dependencies": {
    "Alamofire": [
      "~> 5.9.1"
    ],
    "SDWebImage": [
      "~> 5.19.4"
    ],
    "SwiftLint": [
      "~> 0.55.1"
    ],
    "PromisesSwift": [
      "~> 2.4.0"
    ],
    "IQKeyboardManagerSwift": [
      "~> 7.1.1"
    ],
    "MapboxSearchUI": [
      "~> 1.0.0-rc.8"
    ],
    "MapboxNavigation": [
      "~> 2.18"
    ],
    "SwiftMessages": [
      "~> 10.0.0"
    ]
  },
  "swift_version": "5.0"
}
