{
  "name": "Dalila",
  "version": "1.0.15",
  "summary": "Dalila Module",
  "description": "Dalila iOS App is an application powered by TAM, designed to provide users with a comprehensive platform for various functionalities.",
  "homepage": "https://github.com/tamhub/dalila-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ahmed Hussein": "ahmed.hussein@tamhub.com"
  },
  "platforms": {
    "ios": "16.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "git@github.com:Saudi-Tourism-Authority/Dalila-iOS.git",
    "tag": "1.0.15"
  },
  "source_files": "Dalila/Classes/**/*",
  "resource_bundles": {
    "Dalila": [
      "Dalila/Classes/**/*.xib",
      "Dalila/Resources/**/*.{ttf,xcassets,strings,geojson,json}",
      "Dalila/Configurations/Config-*.plist"
    ]
  },
  "preserve_paths": "Dalila/**/*",
  "info_plist": {
    "BaseDomain": "$(BASE_DOMAIN)",
    "BaseRestDomain": "$(BASE_REST_DOMAIN)",
    "PageSize": "$(PAGE_SIZE)",
    "Port": "$(PORT)",
    "TimeoutInterval": "$(TIMEOUT_INTERVAL)",
    "URLProtocol": "$(PROTOCOL)"
  },
  "dependencies": {
    "Alamofire": [
      "~> 5.9.1"
    ],
    "SDWebImage": [
      "~> 5.19.4"
    ],
    "SwiftLint": [
      "~> 0.55.1"
    ],
    "PromisesSwift": [
      "~> 2.4.0"
    ],
    "IQKeyboardManagerSwift": [
      "~> 7.1.1"
    ],
    "MapboxSearchUI": [
      "~> 1.0.0-rc.8"
    ],
    "MapboxNavigation": [
      "~> 2.18.4"
    ],
    "MapboxCoreNavigation": [
      "~> 2.18.4"
    ],
    "SwiftMessages": [
      "~> 10.0.0"
    ]
  },
  "static_framework": true,
  "swift_version": "5.0"
}
