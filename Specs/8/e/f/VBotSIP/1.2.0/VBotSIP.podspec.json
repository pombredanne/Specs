{
  "name": "VBotSIP",
  "version": "1.2.0",
  "summary": "VBot SIP Library for iOS",
  "description": "Objective-C wrapper around PJSIP.",
  "homepage": "https://vbot.vn",
  "license": "GNU GPL v3",
  "authors": {
    "Dat Nguyen Quoc": "datnq@vpmedia.vn"
  },
  "source": {
    "git": "https://github.com/VBotDevTeam/VBotSIP.git",
    "tag": "1.2.0"
  },
  "social_media_url": "https://vbot.vn",
  "platforms": {
    "ios": "13.5"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*.{h,m}",
