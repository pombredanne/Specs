  "dependencies": {
    "Vialer-pjsip-iOS": [

    ],
    "CocoaLumberjack": [

    ]
  },
  "static_framework": true,
  "ios": {
    "pod_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
    }
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "PJ_AUTOCONF=1"
  },
  "swift_version": "5.0"
}
