{
  "name": "ZJMediaPlayer",
  "version": "1.7.12",
  "summary": "ZJ Meida Player SDK for iOS at CocoaPods.",
  "description": "ZJ Meida Player SDK for iOS at CocoaPods，Supported H.264、Hevc、AAC and other formats.",
  "homepage": "https://github.com/Eafy/ZJMediaPlayerCocoaPads",
  "license": {
    "type": "Apache License 2.0"
  },
  "authors": {
    "Eafy": "lizhijian_21@163.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "arm64"
  },
  "libraries": [
    "c++",
    "z",
    "bz2",
    "iconv"
  ],
  "frameworks": [
    "CoreMedia",
    "VideoToolbox",
    "AudioToolbox",
    "AVFoundation",
    "OpenGLES",
    "GLKit"
  ],
  "dependencies": {
    "ZJLog": [
      "~> 1.2.3"
    ]
  },
  "source": {
    "git": "https://github.com/Eafy/ZJMediaPlayerCocoaPods.git",
    "tag": "v1.7.12"
  },
  "ios": {
    "vendored_frameworks": "ZJMediaPlayer.xcframework"
  }
}
