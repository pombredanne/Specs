{
  "name": "TPTweak",
  "version": "2.0.2",
  "summary": "TPTweak is a debugging tool to help adjust your iOS app on the fly without recompile",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE.md"
  },
  "authors": {
    "Wendy Liga": "wendy.liga@tokopedia.com"
  },
  "homepage": "https://github.com/tokopedia/ios-tptweak",
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": [
    "5.4"
  ],
  "source": {
    "git": "https://github.com/tokopedia/ios-tptweak.git",
    "tag": "2.0.2"
  },
  "source_files": "Sources/TPTweak/**/*.swift",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/TPTweak/"
    },
    {
      "name": "DevTools",
      "compiler_flags": "-DUSE_DEVTOOLS",
      "source_files": "Sources/TPTweak/"
    }
  ],
  "swift_version": "5.4"
}
