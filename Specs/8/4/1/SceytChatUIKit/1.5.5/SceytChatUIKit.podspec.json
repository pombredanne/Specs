{
  "name": "SceytChatUIKit",
  "swift_versions": "5.7",
  "version": "1.5.5",
  "summary": "Fully customizable Chat UI Kit on top of SceytChat SDK.",
  "homepage": "https://sceyt.com",
  "license": "MIT",
  "authors": {
    "Ovsep Keropian": "ovsep@sceyt.com"
  },
  "source": {
    "git": "https://github.com/sceyt/sceyt-chat-ios-uikit.git",
    "tag": "v1.5.5"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "13.0"
  },
  "source_files": [
    "Sources/SceytChatUIKit/**/*.swift"
  ],
  "resources": [
    "Sources/SceytChatUIKit/**/*.xcdatamodeld",
    "Sources/SceytChatUIKit/**/*.plist",
    "Sources/SceytChatUIKit/Resources/**/*.xcassets"
  ],
  "ios": {
    "frameworks": [
      "UIKit",
      "Foundation",
      "CoreData"
    ]
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "SceytChat": [
      ">= 1.5.4"
    ]
  },
  "swift_version": "5.7"
}
