  "homepage": "https://gitee.com/adscope/adscope-adapter.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Cookie": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope/adscope-adapter.git",
    "tag": "5.1.0.2"
  },
  "platforms": {
    "ios": "11.0"
  },
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "Foundation",
    "UIKit"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
