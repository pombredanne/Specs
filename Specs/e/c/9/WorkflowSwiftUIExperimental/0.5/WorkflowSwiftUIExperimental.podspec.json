{
  "name": "WorkflowSwiftUIExperimental",
  "version": "0.5",
  "summary": "Infrastructure for Workflow-powered SwiftUI",
  "homepage": "https://www.github.com/square/workflow-swift",
  "license": "Apache License, Version 2.0",
  "authors": "Square",
  "source": {
    "git": "https://github.com/square/workflow-swift.git",
    "tag": "swiftui-experimental/v0.5"
  },
  "cocoapods_version": ">= 1.7.0",
  "swift_versions": [
    "5.7"
  ],
