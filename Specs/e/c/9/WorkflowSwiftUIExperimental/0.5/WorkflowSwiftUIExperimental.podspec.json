      "source_files": "WorkflowSwiftUIExperimental/Tests/**/*.swift",
      "frameworks": "XCTest",
      "libraries": "swiftos",
      "requires_app_host": true,
      "pod_target_xcconfig": {
        "APPLICATION_EXTENSION_API_ONLY": "NO"
      }
    }
  ],
  "swift_version": "5.7"
