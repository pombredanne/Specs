{
  "name": "AppMetricaCore",
  "version": "5.4.0",
  "summary": "Powerful and flexible module offering a wide range of tracking and analytics tools for your application",
  "homepage": "https://appmetrica.io",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "AppMetrica": "admin@appmetrica.io"
  },
  "source": {
    "git": "https://github.com/appmetrica/appmetrica-sdk-ios.git",
    "tag": "5.4.0"
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreLocation",
    "CoreGraphics",
    "SystemConfiguration"
  ],
  "libraries": [
    "z",
    "sqlite3",
    "c++"
  ],
  "dependencies": {
    "AppMetricaLog": [
      "= 5.4.0"
    ],
    "AppMetricaNetwork": [
      "= 5.4.0"
    ],
    "AppMetricaCoreUtils": [
      "= 5.4.0"
    ],
    "AppMetricaHostState": [
      "= 5.4.0"
    ],
    "AppMetricaProtobufUtils": [
      "= 5.4.0"
    ],
    "AppMetricaPlatform": [
      "= 5.4.0"
    ],
    "AppMetricaStorageUtils": [
      "= 5.4.0"
    ],
    "AppMetricaEncodingUtils": [
      "= 5.4.0"
    ],
    "AppMetricaProtobuf": [
      "= 5.4.0"
    ],
    "AppMetricaFMDB": [
      "= 5.4.0"
    ]
  },
  "header_dir": "AppMetricaCore",
  "source_files": [
    "AppMetricaCore/Sources/**/*.{h,m,c}",
    "AppMetricaCoreExtension/Sources/include/**/*.h"
  ],
  "public_header_files": "AppMetricaCore/Sources/include/**/*.h",
  "resource_bundles": {
    "AppMetricaCore": "AppMetricaCore/Sources/Resources/PrivacyInfo.xcprivacy"
  }
}
