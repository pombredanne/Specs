{
  "name": "XZNavigationController",
  "version": "1.2.5",
  "summary": "iOS 支持全屏手势导航、自定义导航栏的协议框架",
  "description": "拓展了原生 UINavigationController 以支持自定义导航条、全屏手势导航的面向协议的组件",
  "homepage": "https://github.com/Xezun/XZNavigationController",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Xezun": "developer@xezun.com"
  },
  "source": {
    "git": "https://github.com/Xezun/XZNavigationController.git",
    "tag": "1.2.5"
  },
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "XZ_FRAMEWORK=1"
  },
  "swift_versions": "5.0",
  "default_subspecs": "Code",
  "subspecs": [
    {
      "name": "Code",
      "source_files": "XZNavigationController/Code/**/*.{h,m,swift}",
      "public_header_files": "XZNavigationController/Code/**/*.h",
      "dependencies": {
        "XZDefines/XZRuntime": [
          ">= 1.1.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
