{
  "name": "MoguaSDK",
  "version": "0.10.0",
  "summary": "Web to App Parameter Passing Solution.",
  "description": "When a download link for an app is sent via text messages, emails, affiliate pages, QR codes, etc., our SDK tracks the link's parameters when new users download and open the app.",
  "homepage": "https://www.mogua.io",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Omnimind": "omnimind.sg@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/OmnimindSG/mogua-ios-sdk.git",
    "tag": "0.10.0"
  },
  "source_files": "lib/MoguaSDK.framework/**/*.h",
  "public_header_files": "lib/MoguaSDK.framework/Headers/*",
  "vendored_frameworks": "lib/MoguaSDK.framework",
  "preserve_paths": "lib/MoguaSDK.framework",
  "static_framework": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
