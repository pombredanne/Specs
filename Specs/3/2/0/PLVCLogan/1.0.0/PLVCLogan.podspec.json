{
  "name": "PLVCLogan",
  "version": "1.0.0",
  "summary": "PLVCLogan is a lightweight case logging system based on mobile platform.",
  "homepage": "https://help.polyv.net/index.html#/live/ios/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "hamto": "duhan@polyv.net"
  },
  "source": {
    "git": "https://gitee.com/polyvios/plvclogan.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "source_files": "Clogan/*.{h,c}",
  "public_header_files": "Clogan/*.h",
  "subspecs": [
    {
      "name": "mbedtls",
      "source_files": "mbedtls/**/*.{h,c}",
      "header_dir": "mbedtls",
      "private_header_files": "mbedtls/**/*.h",
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/PLVCLogan/**"
      }
    }
  ]
}
